t.Payment_Method,
    
t.City,
    
t.Store_Type,
    
t.Discount_Applied,
    
t.Customer_Category,
    
t.Season,
   
 t.Promotion_Name

FROM dbo.Stg_RetailTransactions t                             
CROSS APPLY STRING_SPLIT(
    REPLACE(REPLACE(t.Product, '[', ''), ']', ''),   -- on enlve les crochets
    ','
) AS split
WHERE split.value IS NOT NULL 
  AND LTRIM(RTRIM(split.value)) &lt;&gt; ''</property>
